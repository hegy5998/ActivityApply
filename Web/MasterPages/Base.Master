    <script type="text/javascript">
        var sessionTimeOutMsg = "登入逾時，請重新登入!";

        // Set jQuery Configure
        $.ajaxSetup({
            cache: false,
            error: function (x, e) {
                if (x.status == 401) {
                    alert(sessionTimeOutMsg);
                }
                window.location.reload();
            }
        });
    </script>

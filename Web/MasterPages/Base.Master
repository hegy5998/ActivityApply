    <style type="text/css">
          body {
            min-width: 100%;
          }
          .column {
            width: 100%;
            float: left;
          }
          .portlet {
            margin: 0 1em 1em 0;
            padding: 0.3em;
            border: 0px solid rgba(170, 170, 170, 0);
          }
          .portlet-header {
            padding: 0.2em 0.3em;
            margin-bottom: 0.5em;
            position: relative;
            border-radius : 20px;
          }
          .portlet-toggle {
            position: absolute;
            top: 50%;
            right: 0;
            margin-top: -8px;
          }
          .portlet-content {
            padding: 0.4em;
          }
          .portlet-placeholder {
            border: 1px dotted black;
            margin: 20px 1em 1em 0;
            height: 100px;
            border-radius : 20px;
          }
          .affix{
              right: 28px;
              top: 90px;
          }
          .red{
              color:red;
          }
  }
    </style>
